{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "qodda"
    },
    "environment": {
      "value": "prod"
    },
    "location": {
      "value": "East US"
    },
    "containerRegistryServer": {
      "value": "qodda.azurecr.io"
    },
    "containerRegistryUsername": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/qodda-rg/providers/Microsoft.KeyVault/vaults/qodda-prod-kv"
        },
        "secretName": "acr-username"
      }
    },
    "containerRegistryPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/qodda-rg/providers/Microsoft.KeyVault/vaults/qodda-prod-kv"
        },
        "secretName": "acr-password"
      }
    }
  }
}
